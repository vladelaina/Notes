<?xml version="1.0" encoding="UTF-8"?>
<DATA>
  <TransformScript>
    <OtherComments>
      <Content>Book to DemiBold High Contrast
This script changes a Book font with lighter horizontal strokes to DemiBold stroke weight. After running it you may need to reduce the weight of serifs and correct intersecting contours.</Content>
    </OtherComments>
    <OutlineScale>
      <AroundType>2</AroundType>
      <AroundCorner>0</AroundCorner>
      <AroundXPos>0</AroundXPos>
      <AroundYPos>0</AroundYPos>
      <Horizontal>97.72</Horizontal>
      <Vertical>97.72</Vertical>
      <PreserveBearings>TRUE</PreserveBearings>
    </OutlineScale>
    <EffectsBold>
      <Horizontal>50</Horizontal>
      <Vertical>17</Vertical>
      <PreserveBearings>TRUE</PreserveBearings>
      <UpdateMetrics>TRUE</UpdateMetrics>
    </EffectsBold>
    <OutlineMove>
      <Horizontal>0</Horizontal>
      <Vertical>17</Vertical>
    </OutlineMove>
    <OutlineOptimize>
    </OutlineOptimize>
    <MetricsLeftSideBearing>
    </MetricsLeftSideBearing>
  </TransformScript>
</DATA>
