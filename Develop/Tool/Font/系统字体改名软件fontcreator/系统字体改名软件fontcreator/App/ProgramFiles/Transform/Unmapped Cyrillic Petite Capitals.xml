      <Vertical>12</Vertical>
      <PreserveBearings>FALSE</PreserveBearings>
      <UpdateMetrics>FALSE</UpdateMetrics>
    </EffectsBold>
    <OutlineMove>
      <Horizontal>0</Horizontal>
      <Vertical>12</Vertical>
    </OutlineMove>
    <GlyphInsertGlyphs>
