<?xml version="1.0" encoding="UTF-8"?>
<DATA>
  <TransformScript>
    <OtherComments>
      <Content>Book to DemiBold
This script changes a Book font to DemiBold stroke weight. 
After running it you may need to reduce the weight of serifs and correct intersecting contours.</Content>
    </OtherComments>
    <OutlineScale>
      <AroundType>2</AroundType>
      <AroundCorner>0</AroundCorner>
      <AroundXPos>0</AroundXPos>
      <AroundYPos>0</AroundYPos>
      <Horizontal>96.92</Horizontal>
      <Vertical>96.92</Vertical>
      <PreserveBearings>TRUE</PreserveBearings>
    </OutlineScale>
    <EffectsBold>
      <Horizontal>50</Horizontal>
      <Vertical>23</Vertical>
      <PreserveBearings>TRUE</PreserveBearings>
      <UpdateMetrics>TRUE</UpdateMetrics>
    </EffectsBold>
    <OutlineMove>
      <Horizontal>0</Horizontal>
      <Vertical>23</Vertical>
    </OutlineMove>
    <MetricsLeftSideBearing>
    </MetricsLeftSideBearing>
  </TransformScript>
</DATA>
