    <EffectsBold>
      <Horizontal>25</Horizontal>
      <Vertical>8</Vertical>
      <PreserveBearings>TRUE</PreserveBearings>
      <UpdateMetrics>FALSE</UpdateMetrics>
    </EffectsBold>
    <OutlineMove>
      <Horizontal>0</Horizontal>
      <Vertical>508</Vertical>
    </OutlineMove>
    <MetricsLeftSideBearing>
    </MetricsLeftSideBearing>
    <GlyphInsertGlyphs>
      <GlyphNames> gravecomb.sups acutecomb.sups Egrave.sups Uacute.sups egrave.sups uacute.sups</GlyphNames>
